{
  "name": "portifolio-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host 192.168.0.104",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "eslint --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .tsx",
    "lint:fix": "eslint --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .tsx --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "vitest --no-watch",
    "test:cover": "vitest --coverage --no-watch",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@open-wc/testing": "^3.1.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "commitizen": "^4.2.6",
    "cypress": "^12.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
